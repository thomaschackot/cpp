#include <iostream>
using namespace std;
class Array
{
  private:
    int *A;
    int size;
    int length;
    void swap(int *x,int *y);

    public:
     Array()
    {
        size=0;
        length=0;
        A=new int[size];
    }
    Array(int sz)
    {
        size=sz;
        length=0;
        A=new int[size];
    }
    ~Array()
    {
        delete []A;
    }
};
 
